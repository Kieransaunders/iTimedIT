{
  "expo": {
    "name": "iTimedIT",
    "slug": "iTimedITAPP",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.itimedit.app",
      "infoPlist": {
        "UIBackgroundModes": [
          "fetch",
          "remote-notification"
        ],
        "CFBundleURLTypes": [
          {
            "CFBundleURLSchemes": [
              "itimeditapp"
            ]
          },
          {
            "CFBundleURLSchemes": [
              "com.googleusercontent.apps.434888822269-fgcqfko6njlc83eqbogltcgaj9ilpeeu"
            ]
          }
        ],
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "icon": "./assets/images/icon.png",
      "package": "com.itimedit.app"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    }
  }
}
